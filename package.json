{
  "name": "mutants",
  "version": "1.0.0",
  "description": "mutants",
  "productName": "mutants",
  "main": "dist/index.js",
  "scripts": {
    "start": "devtools-start",
    "build": "devtools-build",
    "script": "devtools-scripts",
    "dev": "devtools-dev file-path",
    "link": "devtools-scripts link module-name",
    "pub": "npm run build && npm publish --registry https://registry.npmjs.org",
    "test": "set DEBUG=tplus:*,-not_this && mocha"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "mutants",
  "license": "ISC",
  "dependencies": {
    "mutants-microfx": "^1.0.1"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "jest": "^25.0.0",
    "mutants-devtools": "^3.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
